pipeline {
    agent {
        docker { 
          image 'yatin03/amazonlinux:phpapache'
        }
    }
    stages {
        stage('Execute Script') {
            steps {
                sh 'cd Source && ls -lahtr'
                sh 'ls -lahtr'
                sh 'ls -lahtr deployment'
                sh 'ls -lahtr deployment/scripts'
            }
        }
    }
}
